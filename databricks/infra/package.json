{
  "name": "databricks",
  "version": "1.0.0",
  "main": "main.js",
  "types": "main.ts",
  "license": "MPL-2.0",
  "private": true,
  "scripts": {
    "get": "cdktf get",
    "build": "tsc",
    "synth": "cdktf synth",
    "compile": "tsc --pretty",
    "watch": "tsc -w",
    "test": "jest",
    "test:watch": "jest --watch",
    "upgrade": "npm i cdktf@latest cdktf-cli@latest",
    "upgrade:next": "npm i cdktf@next cdktf-cli@next",
    "aws-login": "npx ts-node ../../lib/scripts/aws-login.ts",
    "deploy": "sh deploy.sh"
  },
  "engines": {
    "node": ">=16.0"
  },
  "dependencies": {
    "@cdktf/provider-aws": "^19.15.0",
    "@cdktf/provider-null": "^10.0.0",
    "@cdktf/provider-time": "^10.1.0",
    "@flagright/lib": "file:../../lib",
    "aws-cdk-lib": "~2.141.0",
    "cdktf": "^0.20.7",
    "cdktf-cli": "^0.20.7",
    "constructs": "^10.3.0",
    "prettier": "^3.2.5"
  },
  "devDependencies": {
    "@types/jest": "^29.5.6",
    "@types/node": "^20.8.9",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "lint-staged": {
    "**/*.{js,ts}": [
      "prettier --write"
    ],
    "!(**/*.{js,ts,yaml})": [
      "prettier --write"
    ]
  }
}

{
  "name": "flagright-figma-vars-plugin",
  "version": "0.1.1",
  "files": [
    "build/main.js",
    "build/ui.js",
    "manifest.json"
  ],
  "dependencies": {
    "@create-figma-plugin/ui": "^3.2.0",
    "@create-figma-plugin/utilities": "^3.2.0",
    "@flagright/lib": "link:../lib",
    "clsx": "^2.1.1",
    "lodash": "^4.17.21",
    "preact": ">=10"
  },
  "devDependencies": {
    "@create-figma-plugin/build": "^3.2.0",
    "@create-figma-plugin/tsconfig": "^3.2.0",
    "@figma/plugin-typings": "1.90.0",
    "@types/lodash": "^4",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "prettier": "^3.3.3",
    "typescript": ">=4"
  },
  "scripts": {
    "prepack": "npm run build",
    "postpack": "mv package.tgz flagright-figma-vars-plugin-$npm_package_version.tgz",
    "build": "build-figma-plugin --typecheck --minify",
    "watch": "build-figma-plugin --typecheck --watch",
    "prepare": "husky"
  },
  "figma-plugin": {
    "id": "1403317323142953789",
    "editorType": [
      "figma"
    ],
    "documentAccess": "dynamic-page",
    "name": "Vars Sync Plugin",
    "main": "src/entry-main.ts",
    "ui": "src/entry-ui.tsx",
    "networkAccess": {
      "allowedDomains": [
        "https://api.github.com"
      ]
    },
    "permissions": [
      "currentuser"
    ],
    "enablePrivatePluginApi": true
  },
  "lint-staged": {
    "**/*.{js,jsx,tsx,ts,less,md,json}": [
      "prettier --write"
    ]
  }
}

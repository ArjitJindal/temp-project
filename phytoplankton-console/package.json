{
  "name": "phytoplankton-console",
  "version": "1.0.0",
  "private": true,
  "description": "Flagright console",
  "scripts": {
    "preinstall": "cd ../lib && corepack enable && yarn",
    "aws-login": "npx ts-node ../lib/scripts/aws-login.ts",
    "synth:dev": "ENV=dev cdk synth dev-phytoplankton",
    "synth:dev:user": "ENV=dev:user cdk synth dev-phytoplankton-$QA_SUBDOMAIN",
    "synth:sandbox": "ENV=sandbox cdk synth sandbox-phytoplankton",
    "synth:prod": "ENV=prod cdk synth prod-phytoplankton",
    "deploy:dev": "ENV=dev AWS_MAX_ATTEMPTS=10 AWS_RETRY_MODE=adaptive cdk deploy dev-phytoplankton",
    "deploy:dev:clean": "npm run clear && yarn && npm run build:dev && npm run synth:dev && npm run deploy:dev -- --profile=AWSAdministratorAccess-911899431626",
    "deploy:qa": "npm run clear && yarn && ENV=local npm run build:dev && QA=true QA_SUBDOMAIN=qa$(git rev-parse --abbrev-ref HEAD | shasum | head -c5) ENV=dev:user cdk deploy dev-phytoplankton-qa$(git rev-parse --abbrev-ref HEAD | shasum | head -c5) --require-approval never",
    "destroy:qa": "npm run clear && yarn && ENV=dev npm run build:dev && ENV=dev:user cdk destroy dev-phytoplankton-$QA_SUBDOMAIN --require-approval never -f",
    "deploy:sandbox": "ENV=sandbox cdk deploy sandbox-phytoplankton",
    "deploy:sandbox:clean": "npm run clear && yarn && npm run aws-login sandbox && npm run build:sandbox && npm run synth:sandbox && npm run deploy:sandbox -- --profile=AWSAdministratorAccess-293986822825",
    "deploy:prod": "ENV=prod cdk deploy prod-phytoplankton",
    "deploy:prod:clean": "npm run clear && yarn && npm run aws-login prod && npm run build:prod && npm run synth:prod && npm run deploy:prod -- --profile=AWSAdministratorAccess-870721492449",
    "build:local": "ENV=local npm run openapi:build && cross-env ENV=local node build",
    "build:dev": "npm run openapi:build && cross-env ENV=dev node build",
    "build:sandbox": "npm run openapi:build && cross-env ENV=sandbox node build",
    "build:prod": "npm run openapi:build && cross-env ENV=prod node build",
    "dev": "ENV=local npm run openapi:build && npm run start:local",
    "local:setup": "openssl genrsa -out build/certificates/self_priv.pem 1024 && openssl req -new -key build/certificates/self_priv.pem -out build/certificates/certrequest.csr && openssl x509 -req -in build/certificates/certrequest.csr -signkey build/certificates/self_priv.pem -out build/certificates/self_cert.pem",
    "clear": "rm -rf ./dist cdk.out",
    "gh-pages": "gh-pages -d dist",
    "i18n-remove": "pro i18n-remove --locale=zh-CN --write",
    "lint": "npm run lint:js && npm run lint:style && npm run lint:prettier",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx --fix",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src && npm run lint:style",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:prettier": "prettier -c --write \"src/**/*\" --end-of-line auto",
    "lint:style": "stylelint --fix \"src/**/*.less\" --syntax less",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "openapi:build": "build/openapi-generate.js",
    "precommit": "lint-staged",
    "prettier": "prettier -c --write .",
    "start:local": "ENV=local yarn openapi:build && npm run start:watch",
    "start:dev": "yarn openapi:build && cross-env ENV=dev WATCH=true node build",
    "start:watch": "cross-env ENV=local WATCH=true node build",
    "test:unit": "jest src",
    "tsc": "tsc --noEmit",
    "tsc:watch": "tsc --noEmit --watch",
    "cypress:open": "node cypress-run.js open",
    "cypress:run": "node cypress-run.js run",
    "semgrep": "semgrep scan --config ../rule.yaml --error",
    "test": "npm run tsc && npm run test:unit && npm run format:check",
    "test:coverage": "jest src --silent --coverage && node ../scripts/generate-coverage-stats.js",
    "test:coverage:compare": "OUTPUT_FILE=coverage/coverage-stats.new.json npm run test:coverage && node ../scripts/compare-coverage-stats.js"
  },
  "lint-staged": {
    "**/*.less": "stylelint --syntax less",
    "**/*.{js,jsx,ts,tsx}": [
      "npm run lint-staged:js",
      "npm run semgrep"
    ],
    "**/*.{js,jsx,tsx,ts,less,md,json}": [
      "prettier --write"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],

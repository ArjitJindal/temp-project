{
  "parser": "@typescript-eslint/parser",
  "env": {
    "node": true,
    "es6": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended",
    "plugin:prettier/recommended",
    "plugin:import/recommended",
    "plugin:import/typescript"
  ],
  "rules": {
    "no-console": "off",
    "import/no-unresolved": [
      "error",
      {
        "ignore": ["aws-lambda", "@/*", "@cdk/*"]
      }
    ],
    "import/order": "error"
  }
}

{
  "name": "tarpon",
  "version": "1.0.0",
  "description": "![tarpons](https://github.com/flagright/tarpon/blob/main/tarpons.png)",
  "main": "index.js",
  "scripts": {
    "aws-sso-login": "aws sso login --profile $npm_config_profile && cdk-sso-sync $npm_config_profile",
    "build": "webpack",
    "build:watch": "webpack --watch --progress",
    "synth": "cdk synth",
    "synth:local": "cdk synth local-tarpon",
    "synth:dev": "cdk synth dev-tarpon --profile=AWSAdministratorAccess-911899431626",
    "synth:sandbox": "cdk synth sandbox-tarpon --profile=AWSAdministratorAccess-293986822825",
    "synth:prod": "cdk synth prod-tarpon --profile=AWSAdministratorAccess-870721492449",
    "synth:pipeline": "cdk synth tarpon-pipeline",
    "deploy:dev": "cdk deploy dev-tarpon --profile=AWSAdministratorAccess-911899431626",
    "deploy:sandbox": "cdk deploy sandbox-tarpon --profile=AWSAdministratorAccess-293986822825",
    "deploy:prod": "cdk deploy prod-tarpon --profile=AWSAdministratorAccess-870721492449",
    "deploy:pipeline": "./scripts/deploy-pipeline.sh",
    "test": "ts-mocha **/tests/**/*.ts",
    "format": "prettier --write .",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "openapi:fetch": "bash fetch-openapi.sh",
    "openapi:build:dev": "ENV=dev ./build-openapi.ts",
    "openapi:build:sandbox": "ENV=sandbox ./build-openapi.ts",
    "openapi:build:prod": "ENV=prod ./build-openapi.ts",
    "start-local-api": "sam local start-api -t cdk.out/local-tarpon.template.json",
    "recreate-local-ddb": "aws dynamodb delete-table --endpoint-url http://localhost:8000 --table-name $npm_config_table && aws dynamodb create-table --endpoint-url http://localhost:8000 --table-name $npm_config_table --attribute-definitions AttributeName=PartitionKeyID,AttributeType=S AttributeName=SortKeyID,AttributeType=S --key-schema AttributeName=PartitionKeyID,KeyType=HASH AttributeName=SortKeyID,KeyType=RANGE --provisioned-throughput ReadCapacityUnits=1,WriteCapacityUnits=1 --region=us-east-2 --profile=AWSAdministratorAccess-911899431626",
    "create-local-ddb": "aws dynamodb create-table --endpoint-url http://localhost:8000 --table-name $npm_config_table --attribute-definitions AttributeName=PartitionKeyID,AttributeType=S AttributeName=SortKeyID,AttributeType=S --key-schema AttributeName=PartitionKeyID,KeyType=HASH AttributeName=SortKeyID,KeyType=RANGE --provisioned-throughput ReadCapacityUnits=1,WriteCapacityUnits=1 --region=us-east-2 --profile=AWSAdministratorAccess-911899431626",
    "start-local-ddb": "docker run -p 8000:8000 amazon/dynamodb-local -jar DynamoDBLocal.jar -inMemory -sharedDb",
    "start-local-mongodb": "docker run -p 27017:27017 mongo"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/flagright/tarpon.git"
  },
  "author": "Flagright Data Technologies Inc.",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/flagright/tarpon/issues"
  },
  "homepage": "https://github.com/flagright/tarpon#readme",
  "devDependencies": {
    "@tsconfig/node14": "^1.0.1",
    "@types/ajv": "^1.0.0",
    "@types/auth0": "^2.34.16",
    "@types/aws-lambda": "^8.10.89",
    "@types/chai": "^4.3.0",
    "@types/command-line-args": "^5.2.0",
    "@types/http-errors": "^1.8.2",
    "@types/js-yaml": "^4.0.5",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/lodash": "^4.14.180",
    "@types/mkdirp": "^1.0.2",
    "@types/mocha": "^9.0.0",
    "@types/node": "^17.0.8",
    "@types/uuid": "^8.3.4",
    "@types/webpack": "^5.28.0",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "aws-cdk-lib": "^2.14.0",
    "aws-sdk": "^2.1062.0",
    "chai": "^4.3.4",
    "command-line-args": "^5.2.0",
    "constructs": "^10.0.72",
    "eslint": "^8.6.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "js-yaml": "^4.1.0",
    "mkdirp": "^1.0.4",
    "mocha": "^9.1.3",
    "mock-aws-s3": "^4.0.2",
    "prettier": "^2.5.1",
    "source-map-support": "^0.5.21",
    "ts-loader": "^9.2.6",
    "ts-mocha": "^9.0.2",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.14.1",
    "tsconfig-paths-webpack-plugin": "^3.5.2",
    "typescript": "^4.5.5",
    "webpack": "^5.67.0",
    "webpack-cli": "^4.9.1"
  },
  "dependencies": {
    "@aws-sdk/util-arn-parser": "^3.47.1",
    "@fast-csv/parse": "^4.3.6",
    "ajv": "^8.11.0",
    "auth0": "^2.40.0",
    "base-x": "^3.0.9",
    "copy-webpack-plugin": "^10.2.4",
    "dayjs": "^1.10.7",
    "fp-ts": "^2.11.8",
    "http-errors": "^2.0.0",
    "ibankit": "^1.5.0",
    "io-ts": "^2.2.16",
    "io-ts-reporters": "^2.0.1",
    "jsonwebtoken": "^8.5.1",
    "jwks-rsa": "^2.0.5",
    "lodash": "^4.17.21",
    "mongodb": "^4.4.0",
    "nanoid": "^3.2.0",
    "superagent-proxy": "^3.0.0",
    "unique-names-generator": "^4.6.0",
    "uuid": "^8.3.2"
  }
}

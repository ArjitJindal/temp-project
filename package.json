{
  "name": "tarpon",
  "version": "1.0.0",
  "description": "![tarpons](https://github.com/flagright/tarpon/blob/main/tarpons.png)",
  "main": "index.js",
  "scripts": {
    "aws-sso-login": "aws sso login --profile $npm_config_profile && cdk-sso-sync $npm_config_profile",
    "build": "webpack && cdk synth --profile $npm_config_profile",
    "deploy": "cdk deploy --profile $npm_config_profile",
    "test": "ts-mocha **/tests/**/*.ts",
    "format": "prettier --write .",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "watch": "tsc -w --preserveWatchOutput",
    "openapi:fetch": "bash fetch-openapi.sh",
    "openapi:build": "bash build-openapi.sh",
    "start-local-api": "sam local start-api -t cdk.out/tarpon.template.json",
    "recreate-local-ddb": "aws dynamodb delete-table --endpoint-url http://localhost:8000 --table-name $npm_config_table && aws dynamodb create-table --endpoint-url http://localhost:8000 --table-name $npm_config_table --attribute-definitions AttributeName=PartitionKeyID,AttributeType=S AttributeName=SortKeyID,AttributeType=S --key-schema AttributeName=PartitionKeyID,KeyType=HASH AttributeName=SortKeyID,KeyType=RANGE --provisioned-throughput ReadCapacityUnits=1,WriteCapacityUnits=1",
    "create-local-ddb": "aws dynamodb create-table --endpoint-url http://localhost:8000 --table-name $npm_config_table --attribute-definitions AttributeName=PartitionKeyID,AttributeType=S AttributeName=SortKeyID,AttributeType=S --key-schema AttributeName=PartitionKeyID,KeyType=HASH AttributeName=SortKeyID,KeyType=RANGE --provisioned-throughput ReadCapacityUnits=1,WriteCapacityUnits=1",
    "start-local-ddb": "docker run -p 8000:8000 amazon/dynamodb-local -jar DynamoDBLocal.jar -inMemory -sharedDb",
    "create-user": "ts-node run.ts --action create-user",
    "get-user": "ts-node run.ts --action get-user",
    "create-rule-instance": "ts-node run.ts --action create-rule-instance",
    "update-rule-instance": "ts-node run.ts --action update-rule-instance",
    "verify-transaction": "ts-node run.ts --action verify-transaction",
    "get-transaction": "ts-node run.ts --action get-transaction",
    "import-list": "ts-node run.ts --action import-list",
    "create-and-upload-test-data": "ts-node run.ts --action create-and-upload-test-data --users $npm_config_users --transactions $npm_config_transactions --tenant $npm_config_tenant",
    "import-transaction": "MOCK_S3=true ts-node run.ts --action import-transaction",
    "get-presigned-url": "ts-node run.ts --action get-presigned-url"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/flagright/tarpon.git"
  },
  "author": "Flagright Data Technologies Inc.",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/flagright/tarpon/issues"
  },
  "homepage": "https://github.com/flagright/tarpon#readme",
  "devDependencies": {
    "@aws-cdk/aws-apigateway": "^1.139.0",
    "@aws-cdk/aws-dynamodb": "^1.139.0",
    "@aws-cdk/aws-lambda": "^1.139.0",
    "@aws-cdk/aws-lambda-event-sources": "^1.139.0",
    "@aws-cdk/aws-lambda-nodejs": "^1.139.0",
    "@aws-cdk/core": "^1.139.0",
    "@tsconfig/node14": "^1.0.1",
    "@types/aws-lambda": "^8.10.89",
    "@types/chai": "^4.3.0",
    "@types/command-line-args": "^5.2.0",
    "@types/http-errors": "^1.8.2",
    "@types/js-yaml": "^4.0.5",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/mkdirp": "^1.0.2",
    "@types/mocha": "^9.0.0",
    "@types/node": "^17.0.8",
    "@types/uuid": "^8.3.4",
    "@types/webpack": "^5.28.0",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "aws-sdk": "^2.1062.0",
    "chai": "^4.3.4",
    "command-line-args": "^5.2.0",
    "eslint": "^8.6.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "js-yaml": "^4.1.0",
    "mkdirp": "^1.0.4",
    "mocha": "^9.1.3",
    "mock-aws-s3": "^4.0.2",
    "prettier": "^2.5.1",
    "source-map-support": "^0.5.21",
    "ts-loader": "^9.2.6",
    "ts-mocha": "^9.0.2",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.5",
    "webpack": "^5.67.0",
    "webpack-cli": "^4.9.1"
  },
  "dependencies": {
    "@aws-cdk/aws-docdb": "^1.139.0",
    "@aws-sdk/util-arn-parser": "^3.47.1",
    "@fast-csv/parse": "^4.3.6",
    "base-x": "^3.0.9",
    "dayjs": "^1.10.7",
    "http-errors": "^2.0.0",
    "ibankit": "^1.5.0",
    "io-ts": "^2.2.16",
    "io-ts-reporters": "^2.0.1",
    "jsonwebtoken": "^8.5.1",
    "jwks-rsa": "^2.0.5",
    "nanoid": "^3.2.0",
    "unique-names-generator": "^4.6.0",
    "uuid": "^8.3.2"
  }
}

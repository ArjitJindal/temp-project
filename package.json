{
  "name": "phytoplankton-console",
  "version": "1.0.0",
  "private": true,
  "description": "Flagright console",
  "scripts": {
    "aws-sso-login": "aws sso login --profile $npm_config_profile && cdk-sso-sync $npm_config_profile",
    "aws-sso-login:dev": "npm run aws-sso-login --profile=AWSAdministratorAccess-911899431626",
    "aws-sso-login:sandbox": "npm run aws-sso-login --profile=AWSAdministratorAccess-293986822825",
    "aws-sso-login:prod": "npm run aws-sso-login --profile=AWSAdministratorAccess-870721492449",
    "synth:dev": "ENV=dev cdk synth dev-phytoplankton",
    "synth:sandbox": "ENV=sandbox cdk synth sandbox-phytoplankton",
    "synth:prod": "ENV=prod cdk synth prod-phytoplankton",
    "synth:pipeline": "cdk synth phytoplankton-pipeline",
    "deploy:dev": "ENV=dev cdk deploy dev-phytoplankton",
    "deploy:dev:clean": "npm run clear && yarn && npm run aws-sso-login --profile=AWSAdministratorAccess-911899431626 && npm run build:dev && npm run synth:dev && npm run deploy:dev -- --profile=AWSAdministratorAccess-911899431626",
    "deploy:sandbox": "ENV=sandbox cdk deploy sandbox-phytoplankton",
    "deploy:sandbox:clean": "npm run clear && yarn && npm run aws-sso-login --profile=AWSAdministratorAccess-293986822825 && npm run build:sandbox && npm run synth:sandbox && npm run deploy:sandbox -- --profile=AWSAdministratorAccess-293986822825",
    "deploy:prod": "ENV=prod cdk deploy prod-phytoplankton",
    "deploy:prod:clean": "npm run clear && yarn && npm run aws-sso-login --profile=AWSAdministratorAccess-870721492449 && npm run build:prod && npm run synth:prod && npm run deploy:prod -- --profile=AWSAdministratorAccess-870721492449",
    "deploy:pipeline": "cdk deploy phytoplankton-pipeline --profile=AWSAdministratorAccess-911899431626",
    "deploy:pipeline:clean": "npm run aws-sso-login:dev && npm run synth:pipeline && npm run deploy:pipeline",
    "build:dev": "npm run openapi:build && cross-env ENV=dev node build",
    "build:sandbox": "npm run openapi:build && cross-env ENV=sandbox node build",
    "build:prod": "npm run openapi:build && cross-env ENV=prod node build",
    "dev": "npm run start:dev",
    "clear": "rm -rf ./dist cdk.out",
    "gh-pages": "gh-pages -d dist",
    "i18n-remove": "pro i18n-remove --locale=zh-CN --write",
    "lint": "npm run lint:js && npm run lint:style && npm run lint:prettier && npm run tsc",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src && npm run lint:style",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:prettier": "prettier -c --write \"src/**/*\" --end-of-line auto",
    "lint:style": "stylelint --fix \"src/**/*.less\" --syntax less",
    "openapi:fetch": "build/openapi_fetch.js",
    "openapi:generate-sdk": "build/openapi_generate.sh",
    "openapi:build": "npm run openapi:fetch && npm run openapi:generate-sdk",
    "precommit": "lint-staged",
    "prettier": "prettier -c --write \"src/**/*\"",
    "start:dev": "cross-env ENV=local WATCH=true node build",
    "pretest": "node ./tests/beforeTest",
    "test:all": "node ./tests/run-tests.js",
    "tsc": "tsc --noEmit"
  },
  "lint-staged": {
    "**/*.less": "stylelint --syntax less",
    "**/*.{js,jsx,ts,tsx}": "npm run lint-staged:js",
    "**/*.{js,jsx,tsx,ts,less,md,json}": [
      "prettier --write"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "dependencies": {
    "@ant-design/charts": "^0.9.4",
    "@ant-design/icons": "^4.5.0",
    "@ant-design/pro-components": "^1.1.6",
    "@ant-design/pro-descriptions": "^1.6.8",
    "@ant-design/pro-form": "^1.18.3",
    "@ant-design/pro-layout": "^6.15.3",
    "@ant-design/pro-table": "^2.77.2",
    "@antv/data-set": "^0.11.0",
    "@antv/l7": "^2.3.7",
    "@antv/l7-maps": "^2.3.7",
    "@antv/l7-react": "^2.1.9",
    "@auth0/auth0-react": "^1.9.0",
    "@rjsf/antd": "^4.1.0",
    "@rjsf/core": "^4.1.0",
    "@segment/analytics-next": "^1.38.0",
    "@sentry/react": "^7.2.0",
    "@sentry/tracing": "^7.2.0",
    "@types/react-resizable": "^3.0.2",
    "@umijs/use-request": "^1.4.3",
    "ahooks": "^2.0.0",
    "antd": "4.21.4",
    "axios": "^0.26.0",
    "bizcharts": "^3.5.3-beta.0",
    "bizcharts-plugin-slider": "^2.1.1-beta.1",
    "classnames": "^2.2.6",
    "clsx": "^1.2.1",
    "fast-deep-equal": "^3.1.3",
    "filesize": "^8.0.7",
    "gg-editor": "^2.0.2",
    "html-to-image": "^1.9.0",
    "html2canvas": "^1.4.1",
    "jspdf": "^2.5.1",
    "jspdf-autotable": "^3.5.23",
    "less": "^4.1.3",
    "lodash": "^4.17.11",
    "lodash-decorators": "^6.0.0",
    "moment": "^2.25.3",
    "numeral": "^2.0.6",
    "nzh": "^1.0.3",
    "omit.js": "^2.0.2",
    "react": "^16.8.4",
    "react-dev-inspector": "^1.1.1",
    "react-dom": "^17.0.0",
    "react-fittext": "^1.0.0",
    "react-helmet-async": "^1.0.4",
    "react-resizable": "^3.0.4",
    "react-router": "^6.3.0",
    "react-router-dom": "^6.3.0",
    "umi-request": "^1.4.0",
    "url-parse": "^1.5.10",
    "whatwg-fetch": "^3.6.2"
  },
  "devDependencies": {
    "@ant-design/pro-cli": "^2.0.2",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@openapitools/openapi-generator-cli": "^2.5.1",
    "@tsconfig/node14": "^1.0.1",
    "@types/auth0": "^2.34.17",
    "@types/express": "^4.17.0",
    "@types/history": "^4.7.2",
    "@types/html2canvas": "^1.0.0",
    "@types/jest": "^26.0.0",
    "@types/jspdf": "^2.0.0",
    "@types/lodash": "^4.14.144",
    "@types/lodash.debounce": "^4.0.7",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-helmet": "^6.1.0",
    "@types/url-parse": "^1.4.8",
    "@umijs/fabric": "^2.12.1",
    "@umijs/yorkie": "^2.0.5",
    "aws-cdk": "^2.34.2",
    "aws-cdk-lib": "^2.34.2",
    "carlo": "^0.9.46",
    "constructs": "^10.0.75",
    "cross-env": "^7.0.0",
    "cross-port-killer": "^1.1.1",
    "css-tree": "^2.1.0",
    "detect-installer": "^1.0.1",
    "dotenv": "^16.0.1",
    "enzyme": "^3.11.0",
    "esbuild": "^0.14.47",
    "eslint": "^7.1.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "express": "^4.18.1",
    "express-history-api-fallback": "^2.2.1",
    "fs-extra": "^10.1.0",
    "gh-pages": "^3.0.0",
    "jsdom-global": "^3.0.2",
    "lint-staged": "^10.0.0",
    "mockjs": "^1.0.1-beta3",
    "node-static": "^0.7.11",
    "postcss": "^8.4.14",
    "postcss-less": "^6.0.0",
    "prettier": "^2.3.2",
    "puppeteer-core": "^8.0.0",
    "stylelint": "^13.0.0",
    "tmp": "^0.2.1",
    "ts-node": "^10.8.2",
    "typescript": "^4.2.2",
    "util": "^0.12.4"
  },
  "resolutions": {
    "**/@types/react": "^17.0.0"
  },
  "engines": {
    "node": "16.16.0",
    "npm": "6.14.15"
  }
}

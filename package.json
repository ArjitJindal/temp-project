{
  "name": "tarpon",
  "version": "1.0.0",
  "description": "![tarpons](https://github.com/flagright/tarpon/blob/main/tarpons.png)",
  "main": "index.js",
  "engines": {
    "npm": "^8.6.0",
    "node": "^14.0.0"
  },
  "scripts": {
    "aws-sso-login": "aws sso login --profile $npm_config_profile && cdk-sso-sync $npm_config_profile",
    "build": "webpack",
    "build:watch": "webpack --watch --progress",
    "clear": "rm -rf ./dist cdk.out",
    "synth": "cdk synth",
    "synth:local": "cdk synth local-tarpon",
    "synth:dev": "cdk synth dev-tarpon --profile=AWSAdministratorAccess-911899431626",
    "synth:sandbox": "cdk synth sandbox-tarpon --profile=AWSAdministratorAccess-293986822825",
    "synth:prod": "cdk synth prod-tarpon --profile=AWSAdministratorAccess-870721492449",
    "synth:pipeline": "cdk synth tarpon-pipeline",
    "deploy:dev": "cdk deploy dev-tarpon --profile=AWSAdministratorAccess-911899431626",
    "deploy:dev:clean": "npm run clear && npm install && npm run aws-sso-login --profile=AWSAdministratorAccess-911899431626 && npm run build && npm run openapi:build:dev && npm run synth:dev && npm run deploy:dev",
    "deploy:sandbox": "cdk deploy sandbox-tarpon --profile=AWSAdministratorAccess-293986822825",
    "deploy:sandbox:clean": "npm run clear && npm install && npm run aws-sso-login --profile=AWSAdministratorAccess-293986822825 && npm run build && npm run openapi:build:sandbox && npm run synth:sandbox && npm run deploy:sandbox",
    "deploy:prod": "cdk deploy prod-tarpon --profile=AWSAdministratorAccess-870721492449",
    "deploy:prod:clean": "npm run clear && npm install && npm run aws-sso-login --profile=AWSAdministratorAccess-870721492449 && npm run build && npm run openapi:build:prod && npm run synth:prod && npm run deploy:prod",
    "deploy:pipeline": "./scripts/deploy-pipeline.sh",
    "test": "jest",
    "test:silent": "npm run test -- --silent",
    "test:watch": "npm run test -- --watch",
    "format": "prettier --write .",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "openapi:fetch": "bash fetch-openapi.sh",
    "openapi:build:dev": "ENV=dev ./build-openapi.ts",
    "openapi:build:sandbox": "ENV=sandbox ./build-openapi.ts",
    "openapi:build:prod": "ENV=prod ./build-openapi.ts",
    "start-local-api": "sam local start-api -t cdk.out/local-tarpon.template.json",
    "recreate-local-ddb": "npm run delete-local-ddb --table=$npm_config_table; npm run create-local-ddb --table=$npm_config_table",
    "create-local-ddb": "aws dynamodb create-table --endpoint-url http://localhost:8000 --table-name $npm_config_table --attribute-definitions AttributeName=PartitionKeyID,AttributeType=S AttributeName=SortKeyID,AttributeType=S --key-schema AttributeName=PartitionKeyID,KeyType=HASH AttributeName=SortKeyID,KeyType=RANGE --provisioned-throughput ReadCapacityUnits=1,WriteCapacityUnits=1 --region=us-east-2",
    "delete-local-ddb": "aws dynamodb delete-table --endpoint-url http://localhost:8000 --table-name $npm_config_table",
    "start-local-ddb": "docker run -p 8000:8000 amazon/dynamodb-local -jar DynamoDBLocal.jar -inMemory -sharedDb",
    "start-local-mongodb": "docker run -p 27017:27017 mongo"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/flagright/tarpon.git"
  },
  "author": "Flagright Data Technologies Inc.",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/flagright/tarpon/issues"
  },
  "homepage": "https://github.com/flagright/tarpon#readme",
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "^2.5.1",
    "@tsconfig/node14": "^1.0.1",
    "@types/ajv": "^1.0.0",
    "@types/auth0": "^2.34.16",
    "@types/aws-lambda": "^8.10.89",
    "@types/chai": "^4.3.0",
    "@types/command-line-args": "^5.2.0",
    "@types/flat": "^5.0.2",
    "@types/http-errors": "^1.8.2",
    "@types/jest": "^27.4.1",
    "@types/js-yaml": "^4.0.5",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/lodash": "^4.14.180",
    "@types/mkdirp": "^1.0.2",
    "@types/node": "^17.0.8",
    "@types/node-fetch": "^2.6.1",
    "@types/uuid": "^8.3.4",
    "@types/webpack": "^5.28.0",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "aws-cdk": "^2.23.0",
    "aws-cdk-lib": "^2.14.0",
    "aws-sdk": "^2.1062.0",
    "chai": "^4.3.4",
    "command-line-args": "^5.2.0",
    "constructs": "^10.0.72",
    "copy-webpack-plugin": "^10.2.4",
    "eslint": "^8.6.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.5.1",
    "js-yaml": "^4.1.0",
    "mkdirp": "^1.0.4",
    "mock-aws-s3": "^4.0.2",
    "prettier": "^2.5.1",
    "source-map-support": "^0.5.21",
    "ts-jest": "^27.1.4",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.14.1",
    "tsconfig-paths-webpack-plugin": "^3.5.2",
    "typescript": "^4.5.5",
    "webpack": "^5.67.0",
    "webpack-cli": "^4.9.1"
  },
  "dependencies": {
    "@aws-sdk/util-arn-parser": "^3.47.1",
    "@fast-csv/format": "^4.3.5",
    "@fast-csv/parse": "^4.3.6",
    "ajv": "^8.11.0",
    "auth0": "^2.40.0",
    "base-x": "^3.0.9",
    "dayjs": "^1.10.7",
    "fast-geoip": "^1.1.67",
    "flat": "^5.0.2",
    "fp-ts": "^2.11.8",
    "http-errors": "^2.0.0",
    "ibankit": "^1.5.0",
    "io-ts": "^2.2.16",
    "io-ts-reporters": "^2.0.1",
    "jsonwebtoken": "^8.5.1",
    "jwks-rsa": "^2.0.5",
    "lodash": "^4.17.21",
    "mongodb": "^4.4.0",
    "nanoid": "^3.2.0",
    "node-fetch": "^2.6.7",
    "superagent-proxy": "^3.0.0",
    "unique-names-generator": "^4.6.0",
    "uuid": "^8.3.2"
  }
}
